    ## type = scripted_effect
    ## replace_scopes = { this = country }
    tec_reset_cult_cui_tabs

    ## type = scripted_effect
    ## replace_scopes = { this = country }
    tec_clear_cult_activate_flag

    ## type = scripted_effect
    ## replace_scopes = { this = country }
