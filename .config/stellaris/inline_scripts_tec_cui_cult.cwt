inline_scripts = {
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_button_effect = {
        value[anything] = {
            ## replace_scopes = { this = any root = any from = country }
            potential = single_alias_right[trigger_clause]
            ## replace_scopes = { this = any root = any from = country }
            allow = single_alias_right[trigger_clause]
            ## replace_scopes = { this = any root = any from = country }
            ## cardinality = 0..1
            effect = single_alias_right[effect_clause]
        }
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_activate_cult = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_deactivate_cult = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_clear_activated_flag = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_clear_deactivated_flag = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_clear_cults = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_clear_invalid_cults = {
        ## replace_scopes = { this = country  root = any }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_button_effect_tooltip = {
        ## replace_scopes = { this = any  root = any from = country }
        alias_name[effect] = alias_match_left[effect]
    }
    ## context_configs_type = multiple
    cui/cult/tec_cult_option_is_cult_allowed = {
        ## replace_scopes = { this = country  root = any }
        alias_name[trigger] = alias_match_left[trigger]
    }
}

